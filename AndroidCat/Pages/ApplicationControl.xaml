<Page x:Class="AndroidCat.Pages.ApplicationControl"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AndroidCat.Pages" xmlns:controls="clr-namespace:AndroidCat.Controls"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ApplicationControl">

    <Grid>
        <ScrollViewer VerticalScrollBarVisibility="Auto">
            <WrapPanel>
                <controls:IconTextButton Click="IconTextButton_Click" IconSource="/AndroidCat/emo_im_laughing.png" IconText="安装APK"/>
                <controls:IconTextButton Click="IconTextButton_Click_1" IconSource="/AndroidCat/emo_im_laughing.png" IconText="卸载APK"/>
                <controls:IconTextButton Click="IconTextButton_Click_2" IconSource="/AndroidCat/emo_im_laughing.png" IconText="冻结APK"/>
            </WrapPanel>
        </ScrollViewer>
    </Grid>
</Page>
